{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "diagnostics": {
    "libraryFiles": "Disable"
  },
  "runtime": {
    "version": "LuaJIT",
    "path": [
      "?.lua",
      "?/init.lua"
    ]
  },
  "workspace": {
    "checkThirdParty": "Disable",
    "library": [
      "$VIMRUNTIME/lua",
      "${3rd}/luassert/library",
      "${3rd}/busted/library",
      "../lua",
      "../tests",
      "../.dependencies/pack/vendor/start/plenary.nvim/lua",
      "../.dependencies/pack/vendor/start/nui.nvim/lua",
      "../.dependencies/pack/vendor/start/nvim-web-devicons/lua",
      "../.dependencies/pack/vendor/start/image.nvim/lua",
      "../.dependencies/pack/vendor/start/snacks.nvim/lua",
      "../.dependencies/pack/vendor/start/nvim-window-picker/lua"
    ],
    "ignoreDir": [
      ".dependencies",
      ".luarocks",
      ".lua",
      ".repro"
    ]
  }
}
